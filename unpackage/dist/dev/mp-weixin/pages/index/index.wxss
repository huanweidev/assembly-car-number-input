.parkingFee {
  padding: 0 30rpx;
}
.parkingFee .content .content-top {
  padding-bottom: 30rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.parkingFee .content .content-top .title {
  font-size: 36rpx;
  font-weight: bold;
  color: #333333;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}
.parkingFee .content .content-top .switch {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-size: 28rpx;
  font-weight: 500;
  color: #009D7A;
}
.parkingFee .content .content-top .switch ._img {
  width: 30rpx;
  height: 24rpx;
  margin-right: 17rpx;
}
.parkingFee .content .content-bottom {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.parkingFee .content .content-bottom .item {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  border: 2rpx solid #EEEEEE;
  font-size: 36rpx;
  font-weight: 500;
  color: #333333;
  border-right: 0;
}
.parkingFee .content .content-bottom .item:nth-child(2) {
  margin-right: 20rpx;
}
.parkingFee .content .content-bottom .item:nth-child(2), .parkingFee .content .content-bottom .item:last-child {
  border-right: 2rpx solid #EEEEEE;
}
.parkingFee .content .content-bottom .item.checked {
  border: 2rpx solid #009D7A;
}
.parkingFee .content .btn {
  height: 80rpx;
  line-height: 80rpx;
  width: 472rpx;
  text-align: center;
  background: -webkit-linear-gradient(bottom, #009D7A 0%, #00C89C 100%);
  background: linear-gradient(0deg, #009D7A 0%, #00C89C 100%);
  box-shadow: 0px 14rpx 21rpx 0rpx rgba(10, 160, 127, 0.28);
  border-radius: 40rpx;
  font-size: 28rpx;
  font-weight: bold;
  color: #FFFFFF;
  margin: 120rpx auto 0;
}
.parkingFee .content .btn.dis {
  background: -webkit-linear-gradient(bottom, #009D7A 0%, #00C89C 100%);
  background: linear-gradient(0deg, #009D7A 0%, #00C89C 100%);
  box-shadow: 0rpx 14rpx 21rpx 0rpx rgba(10, 160, 127, 0.28);
  opacity: 0.5;
}
